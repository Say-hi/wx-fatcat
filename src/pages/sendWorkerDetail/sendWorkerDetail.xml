<view class="d-top f28">
  <view class="iconfont icon-xianxiamendian c333">阿克苏东方红卡死的风景卡</view>
  <view class="c666">订单数量:23 <text class="f22 {{ status != 1 ? 'arrive' : 'onroad' }}">asdf</text></view>
</view>

<view wx:for="{{ menuArr }}" wx:for-item="oItem" wx:key="*325" class="p20 out-g bd20">
  <view class="f24 c999 lh44">订单编号: {{ oItem.orderNum }}</view>
  <view class="goods" wx:for="{{ oItem.arr }}" wx:key="*9879">
    <image src="{{ item.img }}"></image>
    <view class="g-i">
      <view class="f30 c333">{{ item.name }}</view>
      <view class="g-b">
        <view class="f30 c333">
          <text class="f30">￥{{ item.salePrice }}　</text>
          <text class="f22 c999 through-line">￥{{ item.salePrice }}</text>
        </view>
        <view class="f24 c999">×{{ item.num }}</view>
      </view>
    </view>
  </view>
</view>